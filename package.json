{
  "name": "de",
  "version": "1.0.0",
  "scripts": {
    "dev:apps": "pnpm -r --parallel --filter=./apps/* run dev",
    "dev:packages": "pnpm -r --parallel --filter=./packages/* run dev",
    "build:apps": "pnpm -r --parallel --filter=./apps/* run build",
    "build:packages": "pnpm -r --parallel --filter=./packages/* run build",
    "format": "prettier --check --ignore-unknown '**/*.{ts,js,tsx,css}'",
    "format:fix": "prettier --write --ignore-unknown '**/*.{ts,js,tsx,css}'"
  },
  "devDependencies": {
    "@crashmax/prettier-config": "^1.1.1",
    "@crashmax/tsconfig": "^1.0.1",
    "@types/node": "18.0.0",
    "@types/react": "^18.0.14",
    "@vitejs/plugin-react": "^1.3.2",
    "tsx": "^3.7.0",
    "typescript": "4.7.4",
    "vite": "^2.9.13",
    "vite-plugin-dts": "^1.2.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "pnpm@4.4.1",
  "prettier": "@crashmax/prettier-config",
  "volta": {
    "node": "18.4.0"
  }
}
