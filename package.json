{
  "name": "de",
  "version": "1.0.0",
  "scripts": {
    "dev:apps": "pnpm -r --parallel --filter=./apps/* run dev",
    "dev:packages": "pnpm -r --parallel --filter=./packages/* run dev",
    "build:apps": "pnpm -r --parallel --filter=./apps/* run build",
    "build:packages": "pnpm -r --parallel --filter=./apps/* run build",
    "format": "prettier --check --ignore-unknown 'src/**/*.{ts,tsx,css}'",
    "format:fix": "prettier --write --ignore-unknown 'src/**/*.{ts,tsx,css}'"
  },
  "devDependencies": {
    "@crashmax/prettier-config": "^1.1.1",
    "@types/node": "18.0.0",
    "typescript": "4.7.4"
  },
  "packageManager": "pnpm@4.4.1",
  "prettier": "@crashmax/prettier-config",
  "volta": {
    "node": "18.4.0"
  }
}
